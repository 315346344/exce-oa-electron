    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.13.1",
    "axios": "^0.21.1",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.3.9",
    "emoji-mart": "^3.0.1",
    "i18next": "^19.8.9",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.7",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.1.2",
    "umi-request": "^1.3.5",
